{
  "name": "@goodrequest/passport-jwt-wrapper",
  "version": "1.1.0-beta",
  "description": "[![Build and run tests](https://github.com/Slonik923/passport-jwt-wrapper/actions/workflows/test.yaml/badge.svg)](https://github.com/Slonik923/passport-jwt-wrapper/actions/workflows/test.yaml) [![codecov](https://codecov.io/gh/Slonik923/passport-jwt-wrapper/branch/master/graph/badge.svg?token=5CXS6JGCQF)](https://codecov.io/gh/Slonik923/passport-jwt-wrapper) [![Publish package to GitHub Packages](https://github.com/Slonik923/passport-jwt-wrapper/actions/workflows/publish.yaml/badge.svg)](https://github.com/Slonik923/passport-jwt-wrapper/actions/workflows/publish.yaml)",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "scripts": {
    "test": "JWT_SECRET=somerandomsecret ts-mocha --config \"./.mocharc.js\"",
    "test:debug": "JWT_SECRET=somerandomsecret ts-mocha --config \"./.mocharc.js\" --inspect",
    "test:coverage": "JWT_SECRET=somerandomsecret nyc --reporter=lcov --temp-dir=\"./temp/.nyc_output\" ts-mocha --config \"./.mocharc.js\" --timeout=100000",
    "lint": "eslint src tests --color --ext .js --ext .ts",
    "lint:fix": "eslint src tests --color --ext .js --ext .ts --fix",
    "lint:watch": "npm run lint -- --watch  --ext .js --ext .ts",
    "translate:scan": "i18next-scanner 'src/**/*.ts'",
    "prebuild": "rm -rf dist",
    "build": "tsc",
    "start:prod": "node dist/index.js",
    "start": "ts-node src/index.ts",
    "start:dev": "ts-node src/index.ts --watch"
  },
  "keywords": [],
  "author": "Juraj Chripko <juraj.chripko@goodrequest.com>",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GoodRequest/passport-jwt-wrapper.git"
  },
  "devDependencies": {
    "@goodrequest/eslint-config-typescript": "^1.2.0",
    "@types/bcrypt": "^5.0.0",
    "@types/chai": "^4.3.4",
    "@types/config": "^3.3.0",
    "@types/express": "^4.17.15",
    "@types/i18next": "^13.0.0",
    "@types/joi": "^17.2.3",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.11.17",
    "@types/passport": "^1.0.11",
    "@types/passport-jwt": "^3.0.8",
    "@types/passport-local": "^1.0.34",
    "@types/sequelize": "^4.28.14",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^5.46.1",
    "chai": "^4.3.7",
    "eslint": "8.30.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "i18next-fs-backend": "^2.1.0",
    "i18next-http-middleware": "^3.2.1",
    "i18next-scanner": "^4.1.0",
    "mocha": "^10.2.0",
    "node-mocks-http": "^1.12.1",
    "nyc": "^15.1.0",
    "passport": "^0.6.0",
    "prettier": "^2.8.1",
    "supertest": "^6.3.3",
    "ts-mocha": "^10.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "bcrypt": "^5.1.0",
    "config": "^3.3.8",
    "express": "^4.18.2",
    "i18next": "^22.4.6",
    "joi": "^17.7.0",
    "jsonwebtoken": "^8.5.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "uuid": "^9.0.0"
  },
  "bugs": {
    "url": "https://github.com/GoodRequest/passport-jwt-wrapper/issues"
  },
  "homepage": "https://github.com/GoodRequest/passport-jwt-wrapper#readme",
  "directories": {
    "test": "tests"
  }
}
